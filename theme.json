{
  "$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/theme.json",
  "version": 2,

  "settings": {
    "color": {
      "defaultPalette": false,
      "palette": [
        {
          "slug": "comp-white",
          "color": "rgb(255,255,255)",
          "name": "Complete White"
        },
        {
          "slug": "comp-gray-100",
          "color": "rgb(243 244 246)",
          "name": "Complete Gray 100"
        },
        {
          "slug": "comp-gray-200",
          "color": "rgb(229 231 235)",
          "name": "Complete Gray 200"
        },
        {
          "slug": "comp-gray-300",
          "color": "rgb(209 213 219)",
          "name": "Complete Gray 300"
        },
        {
          "slug": "comp-gray-400",
          "color": "rgb(156 163 175)",
          "name": "Complete Gray 400"
        },
        {
          "slug": "comp-gray-500",
          "color": "rgb(107 114 128)",
          "name": "Complete Gray 500"
        },
        {
          "slug": "comp-gray-600",
          "color": "rgb(75 85 99)",
          "name": "Complete Gray 600"
        },
        {
          "slug": "comp-gray-700",
          "color": "rgb(55 65 81)",
          "name": "Complete Gray 700"
        },
        {
          "slug": "comp-gray-800",
          "color": "rgb(31 41 55)",
          "name": "Complete Gray 800"
        },
        {
          "slug": "comp-gray-900",
          "color": "rgb(17 24 39)",
          "name": "Complete Gray 900"
        },
        {
          "slug": "comp-primary",
          "color": "rgb(239 68 68)",
          "name": "Complete Primary"
        },
        {
          "slug": "comp-accent",
          "color": "rgb(67 56 202)",
          "name": "Complete Accent"
        },
        {
          "slug": "comp-success",
          "color": "rgb(52 211 153)",
          "name": "Complete Success"
        },
        {
          "slug": "comp-info",
          "color": "rgb(96 165 250)",
          "name": "Complete Info"
        },
        {
          "slug": "comp-warning",
          "color": "rgb(251 146 60)",
          "name": "Complete Warning"
        },
        {
          "slug": "comp-danger",
          "color": "rgb(236 72 153)",
          "name": "Complete Danger"
        }
      ],
      "background": true,
      "text": true,
      "link": true,
      "custom": true,
      "duotone": [
        {
          "slug": "comp-pink-sunset",
          "colors": ["#11245e", "#dc4379"],
          "name": "Complete Pink Sunset"
        }
      ],
      "defaultGradients": false,
      "gradients": [
        {
          "slug": "comp-summer-dog",
          "name": "Complete Summer Dog",
          "gradient": "linear-gradient(#a8ff78,#78ffd6)"
        }
      ]
    },
    "blocks": {
      "core/site-title": {
        "color": {
          "defaultPalette": true,
          "palette": [
            {
              "slug": "comp-primary",
              "color": "#ef4444",
              "name": "Complete Primary"
            }
          ],
          "link": true,
          "custom": false,
          "gradients": [],
          "customGradient": false
        }
      },
      "core/cover": {
        "color": {
          "duotone": [],
          "customDuotone": true
        }
      }
    },
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    }
  },

  "styles": {
    "color": {
      "text": "var(--wp--preset--color--comp-gray-700)",
      "background": "var(--wp--preset-color-comp-white)"
    },
    "blocks": {
      "core/button": {
        "color": {
          "gradient": "var(--wp--preset--gradient--comp-summer-dog)"
        }
      },
      "core/pullquote": {
        "border": {
          "width": "2px",
          "radius": "10px",
          "style": "solid",
          "color": "var(--wp--preset--color--comp-primary)"
        }
      }
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--comp-primary)"
        }
      }
    }
  }
}
